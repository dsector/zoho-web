{{#get-started-slider action="setSlide" prevSlide="address" nextSlide="solar-production" slideName="energy-efficiency"}}
    <h3>Energy Efficiency</h3>
    <hr/>
    {{#unless proposal.items.length}}
      Please select a <a {{action "setSlide" "lookup"}}>market profile</a>.
    {{/unless}}

  {{#each item in proposal.items}}
    <div class="form-group">
        <label>
            {{input type="checkbox" checked=item.selected}}
            {{item.product.name}}
        </label>
    </div>
    {{#if item.selected}}
      <div class="col-md-4">
        {{item-form item=item}}
      </div>
    {{/if}}
    <div class="clearfix"></div>
  {{/each}}
{{/get-started-slider}}