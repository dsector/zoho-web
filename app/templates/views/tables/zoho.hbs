<table id="zoho-table" class="table table-striped table-bordered table-hover" width="100%">
    <thead>
    <tr>
        <th data-class="expand"><i class="fa fa-fw fa-user text-muted hidden-md hidden-sm hidden-xs"></i> Owner</th>
        <th data-hide="phone"> Probability</th>
        <th>Company</th>
        <th data-hide="phone,tablet"><i class="fa fa-fw txt-color-blue hidden-md hidden-sm hidden-xs"></i>
            Zip
        </th>
        <th data-hide="phone,tablet">City</th>
        <th data-hide="phone,tablet"><i class="fa fa-fw fa-calendar txt-color-blue hidden-md hidden-sm hidden-xs"></i>
            Date
        </th>
    </tr>
    </thead>
    <tbody>
    {{#each potential in potentials}}
        <tr>
            <td>{{potential.owner}}</td>
            <td>{{potential.probability}}%</td>
            <td>Et Rutrum Non Associates</td>
            <td>35728</td>
            <td>Fogo</td>
            <td>03/04/14</td>
        </tr>
    {{/each}}
    </tbody>
</table>